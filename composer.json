{
  "name": "helppc/czech-data-box",
  "homepage": "http://helppc.cz",
  "description": "PHP library for connection to Integrated system of data box (ISDS) in Czech republic",
  "keywords": [
    "isds",
    "datovky",
    "czech"
  ],
  "time": "2018-09-08",
  "type": "library",
  "support": {
    "email": "info@tirus.cz",
    "issues": "https://gitlab.com/helppc/czechDataBox/issues",
    "source": "https://gitlab.com/helppc/czechDataBox/"
  },
  "license": [
    "BSD-3-Clause",
    "GPL-2.0",
    "GPL-3.0"
  ],
  "authors": [
    {
      "name": "Tomas Kulhanek",
      "homepage": "http://tirus.cz"
    },
    {
      "name": "Vojtech Krupka"
    }
  ],
  "require": {
    "php": ">=7.3",
    "ext-xml": "*",
    "ext-curl": "*",
    "ext-openssl": "*",
    "ext-dom": "*",
    "guzzlehttp/guzzle": "^6.3",
    "doctrine/collections": "^1.5",
    "symfony/yaml": "^4.2",
    "helppc/serializer": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "HelpPC\\CzechDataBox\\": "src/"
    }
  },
  "scripts": {
    "phpstan": "phpstan analyse -c phpstan.neon --memory-limit=1024M",
    "tester": "tester -C tests/php.ini tests"
  },
  "suggest": {
    "ext-curl": "for comunication with czech databox servers"
  },
  "require-dev": {
    "phpstan/phpstan": "^0.11.8",
    "phpstan/phpstan-nette": "^0.11.1",
    "nette/tester": "^2.0"
  },
  "archive": {
    "exclude": [
      "/tests"
    ]
  }
}
